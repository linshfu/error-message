    "build:dev": "NODE_ENV=production webpack src/ErrorMessage.js dist/errormessage.js -w",
    "build:umd": "NODE_ENV=production webpack src/ErrorMessage.js dist/errormessage.js",
    "build:umd:min": "NODE_ENV=production webpack src/ErrorMessage.js dist/errormessage.min.js -p",
