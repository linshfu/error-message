{
  "name": "error-message",
  "version": "1.0.7",
  "description": "error-message",
  "main": "dist/errormessage.js",
  "scripts": {
    "build": "npm run clean && npm run build:umd && npm run build:umd:min",
    "build:dev": "NODE_ENV=production webpack src/ErrorMessage.js dist/errormessage.js -w",
    "build:umd": "NODE_ENV=production webpack src/ErrorMessage.js dist/errormessage.js",
    "build:umd:min": "NODE_ENV=production webpack src/ErrorMessage.js dist/errormessage.min.js -p",
    "build:index": "npm run clean && NODE_ENV=production webpack src/ErrorMessage.js ./index.js",
    "dev": "npm run clean && npm run build:umd && npm run demo",
    "clean": "rm -rf dist",
    "demo": "browser-sync start --serveStatic example dist",
    "test": "webpack-dashboard -- nodemon --config nodemon.json ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linshfu/error-message.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linshfu/error-message/issues"
  },
  "homepage": "https://github.com/linshfu/error-message#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.22.0",
    "browser-sync": "^2.14.0",
    "express": "^4.14.1",
    "html-webpack-plugin": "^2.28.0",
    "lodash.get": "^4.4.2",
    "mocha": "^3.0.2",
    "nodemon": "^1.11.0",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "whatwg-fetch": "^2.0.2"
  }
}
